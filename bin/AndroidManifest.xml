<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.main"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/algorithm_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.main.MainActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.main.DesignActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_model"
            android:parentActivityName="com.example.main.MainActivity" >
        </activity>
        <activity
            android:name="com.example.main.PaintActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_paint"
            android:parentActivityName="com.example.main.DesignActivity" >
        </activity>
        <activity
            android:name="com.example.main.AnimationActivity"
            android:label="@string/title_activity_animation" >
        </activity>
        <activity
            android:name="com.example.main.RunFragment"
            android:label="@string/title_activity_run" >
        </activity>
        <activity
            android:name="com.example.main.JumpFragment"
            android:label="@string/title_activity_jump" >
        </activity>
        <activity
            android:name="com.example.main.DownFragment"
            android:label="@string/title_activity_down" >
        </activity>
        <activity
            android:name="com.example.main.AttackFragment"
            android:label="@string/title_activity_attack" >
        </activity>
    </application>

</manifest>
